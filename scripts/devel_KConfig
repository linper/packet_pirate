
menu "Development"

config DFN_DEVEL_BUF_SIZE
int "Buff size"
default 512
help
	Most common internal buffer size

config DEVEL_COMP_FLAGS
string "Compilation flags"
default "-Og -ggdb"

config DFN_DEVEL_SANITY
bool "Sanity checks"
default y

config DFN_DEVEL_STASH_DEBUG
bool "Stash debug"
default n

config DFN_DEVEL_TAG_LEN
int "Tag length"
default 16
help
	Default tag buffer size

endmenu
